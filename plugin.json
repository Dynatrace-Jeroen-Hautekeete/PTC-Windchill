		"splitting": {
				"name": "Name",
				"type": "keyProperty",
				"keyProperty": "Name"
		},
        "attribute": "WaitingReadyEntries",
        "allowAdditionalKeys": true,
        "calculateDelta": false,
        "calculateRate": false,
        "aggregation": "AVG"
